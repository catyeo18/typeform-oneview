{
  "name": "Typeform Oneview",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "See all questions in a Typeform survey in one view.",
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_popup": "index.html"
  },
  "content_scripts": [{
    "js": ["content.js"],
    "matches": ["https://form.typeform.com/*"]
  }]
}
